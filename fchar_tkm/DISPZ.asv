function [Zo]=dispz(Zi,kf);
% |Преобразует коэфф. полиномов в строках матрицы Zi для изменения
% характерных псевдочастот в kf раз. Число строк Zi должно быть чётным.
% (В строках Zi задаются последовательно числитель, знаменатель)|

[n,m]=size(Zi);   
for s=2:2:fix(n/2)*2
        W=refzw(Zi(s-1,:),Zi(s,:));
        Wd=dispom(W,kf);
        Zo(s-1:s,:)=refwz(Wd(1,:),Wd(2,:));
end
